FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

//Mesh A
//endTime         25.0;

// Mesh A
//deltaT          0.01;

//Mesh B
endTime         120.0;

// Mesh B
deltaT          0.01;

writeControl    timeStep; 

writeInterval   200.0;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


// ************************************************************************* //

functions 
{ 
    #includeFunc  probes 
    #includeFunc  singleGraph 
}
